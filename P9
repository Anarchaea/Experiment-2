#include <iostream>
#include <conio.h>

using namespace std;

int main()

{
int rows;
int columns;
int r;
int c;

cout << "Enter number of rows: ";
cin >> rows;

cout << "Enter number of columns: ";
cin >>columns;

for(r = 0; r < rows; r++)
{
	for(c = columns; c > 0; c--)
		{
			cout << "*";}
	cout << endl;
	}
	_getch();	
	return 0;
}
